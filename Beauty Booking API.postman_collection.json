        "exec": [
          "const productKey = pm.variables.get(\"productKey\");\r",
          "const secretKey  = pm.variables.get(\"secretKey\");\r",
          "const timestamp  = Math.floor(Date.now() / 1000).toString();\r",
          "\r",
          "let queryParams = {};\r",
          "let bodyParams  = {};\r",
          "\r",
          "// Query\r",
          "pm.request.url.query.each(q => {\r",
          "    if (q.disabled) return;\r",
          "    queryParams[q.key] = q.value;\r",
          "});\r",
          "\r",
          "// Bodyï¼ˆæ”¯æŒä¸‰ç§æ¨¡å¼ + å˜é‡æ›¿æ¢ï¼‰\r",
          "if (pm.request.body) {\r",
          "    if (pm.request.body.mode === 'raw') {\r",
          "        let rawBody = pm.variables.replaceIn(pm.request.body.raw || '');\r",
          "        try {\r",
          "            bodyParams = JSON.parse(rawBody || '{}');\r",
          "        } catch {\r",
          "            bodyParams = {};\r",
          "        }\r",
          "    }\r",
          "    else if (pm.request.body.mode === 'urlencoded') {\r",
          "        pm.request.body.urlencoded.each(p => bodyParams[p.key] = pm.variables.replaceIn(p.value));\r",
          "    }\r",
          "    else if (pm.request.body.mode === 'formdata') {\r",
          "        bodyParams = {};\r",
          "        pm.request.body.formdata.each(p => {\r",
          "            if (p.disabled) return;\r",
          "            let value = pm.variables.replaceIn(p.value);\r",
          "            let keyParts = p.key.split(/\\[|\\]/).filter(k => k);\r",
          "            let ref = bodyParams;\r",
          "\r",
          "            for (let i = 0; i < keyParts.length; i++) {\r",
          "                let part = keyParts[i];\r",
          "                let isLast = i === keyParts.length - 1;\r",
          "\r",
          "                if (isLast) {\r",
          "                    if (/\\[\\]$/.test(p.key) || part === '') {\r",
          "                        if (!Array.isArray(ref[part])) ref[part] = [];\r",
          "                        ref[part].push(value);\r",
          "                    } else {\r",
          "                        ref[part] = value;\r",
          "                    }\r",
          "                } else {\r",
          "                    if (!ref[part]) {\r",
          "                        if (keyParts[i + 1] === '' || /\\[\\]$/.test(p.key)) {\r",
          "                            ref[part] = [];\r",
          "                        } else {\r",
          "                            ref[part] = {};\r",
          "                        }\r",
          "                    }\r",
          "                    ref = ref[part];\r",
          "                }\r",
          "            }\r",
          "        });\r",
          "    }\r",
          "}\r",
          "\r",
          "// ===== æ ¼å¼ä¿®æ­£é€»è¾‘ =====\r",
          "if (bodyParams.coupons && typeof bodyParams.coupons === 'object' && !Array.isArray(bodyParams.coupons)) {\r",
          "    bodyParams.coupons = Object.values(bodyParams.coupons);\r",
          "}\r",
          "\r",
          "if (bodyParams.products && typeof bodyParams.products === 'object' && !Array.isArray(bodyParams.products)) {\r",
          "    bodyParams.products = Object.values(bodyParams.products);\r",
          "}\r",
          "\r",
          "// Merge + Sort\r",
          "let mergedParams = Object.assign({}, queryParams, bodyParams);\r",
          "\r",
          "// ğŸ”¥ å…³é”®ä¿®å¤ï¼šå¦‚æœæ²¡æœ‰ä»»ä½•å‚æ•°ï¼Œbody åº”è¯¥æ˜¯ç©ºå­—ç¬¦ä¸²ï¼Œè€Œä¸æ˜¯ \"{}\"\r",
          "let body;\r",
          "if (Object.keys(mergedParams).length === 0) {\r",
          "    body = \"\";\r",
          "} else {\r",
          "    let sortedKeys   = Object.keys(mergedParams).sort();\r",
          "    let sortedParams = {};\r",
          "    sortedKeys.forEach(k => sortedParams[k] = mergedParams[k]);\r",
          "    body = JSON.stringify(sortedParams);\r",
          "}\r",
          "\r",
          "const signString = productKey + secretKey + timestamp + body;\r",
          "const signature  = CryptoJS.MD5(signString).toString();\r",
          "\r",
          "\r",
          "// Headers\r",
          "pm.request.headers.upsert({ key: \"X-Product-Key\", value: productKey });\r",
          "pm.request.headers.upsert({ key: \"X-Timestamp\",   value: timestamp });\r",
          "pm.request.headers.upsert({ key: \"X-Signature\",   value: signature });\r",
          "\r",
          "// ğŸ”¥ å¯é€‰ï¼šå¦‚æœä½ æƒ³ä¿ç•™ X-Body ç”¨äºè°ƒè¯•ï¼Œå¯ä»¥ä¿ç•™ï¼›å¦åˆ™å»ºè®®åˆ é™¤\r",
          "// pm.request.headers.upsert({ key: \"X-Body\", value: body });"
        ]
      }
